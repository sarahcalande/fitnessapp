<main class="group content">
  Add a food to your diary

  <form id="foodForm"
    action="<%= search_daily_entry_foods_url(@entry) %>"
    method="get"
    class="group">

    <%= form_auth %>
    <input type="text" name="food[name]">
    <p><input class="button" type="submit" value="Find foods"></p>
  </form>

  <p>Entry ID: <%= @entry %></p>
</main>rout