<main class="group content" id="food-diary">
  <%= render 'shared/errors' %>
  <%= render 'shared/flashes' %>

  <h1>Today's food diary</h1>

  <%= render 'shared/diary_entry', date: Date.today %>

  <%= render 'diary/diary_totals', user: @user, entry: @entry, date: Date.today %>

  <section id="archive">
    <h1>Find an archived entry</h1>
    <%= render 'diary/archived_entry_form' %>
  </section>

</main>